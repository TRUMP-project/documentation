prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

prefix mp: <http://stardog.clariah-sdh.eculture.labs.vu.nl/databases/trump/vocab/>
prefix trumpres: <http://stardog.clariah-sdh.eculture.labs.vu.nl/databases/trump/resource/>
PREFIX sdmx: <http://purl.org/linked-data/sdmx/2009/code#>
prefix dbr: <http://dbpedia.org/resource/>
prefix dbo: <http://dbpedia.org/ontology/>


# Select Countries, with their labels, a time period and the values of their indicators at that timeperiod.
# ValueP = Population Value
# ValueU = Unemployment Value (rate)

select distinct ?Country ?Label ?Time_Period ?ValueP ?ValueU ?From_Country ?To_Country ?InflowValue ?Inflow_Time_Period

where {
  
    ?Country a dbo:Country ;
			 rdfs:label ?Label ;
			 mp:population ?Population ;
  			 mp:unemployment_rate ?Unempoyment_rate .
	  
  	graph trumpres:population_eu_graph{
    ?Population mp:country ?Country ;  
    			mp:time_period ?Time_Period ;
     		 	mp:population_value ?ValueP .
    }
          
  	graph trumpres:unemployment_eu_graph{
    ?Unemployment_rate mp:country ?Country ; 
    				   mp:time_period ?Time_Period ;
                       mp:unit	"Active population"^^xsd:string ;
     		 		   mp:indicator_value ?ValueU .
    }
  
	optional{
      graph trumpres:inflow_graph{
        ?Inflow mp:from_country ?From_Country ;
                mp:to_country ?To_Country ;
                mp:movement_time_period ?Inflow_Time_Period ;
                mp:movement_value ?InflowValue .    
        filter(?To_Country = ?Country) 
        
      }}                   
    
# Somewhere here we need to say : if ?Country = ?From_Country make it "country of origin" and To_Country
       
}order by (?Country) (?Time_Period)