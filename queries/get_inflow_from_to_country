prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
prefix owl: <http://www.w3.org/2002/07/owl#>
prefix xsd: <http://www.w3.org/2001/XMLSchema#>
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>

prefix mp: <http://stardog.clariah-sdh.eculture.labs.vu.nl/databases/trump/>
prefix trumpres: <http://stardog.clariah-sdh.eculture.labs.vu.nl/databases/trump/resource/>
prefix trumpvoc: <http://stardog.clariah-sdh.eculture.labs.vu.nl/databases/trump/vocab/>
PREFIX sdmx: <http://purl.org/linked-data/sdmx/2009/code#>

select distinct ?From_Country ?To_Country ?Value ?Time_period
where { 
 
	?Inflow trumpvoc:from_country ?From_Country .
  	?Inflow trumpvoc:to_country ?To_Country .
  	?Inflow trumpvoc:movement_value ?Value .
  	?Inflow trumpvoc:time_period ?Time_period .

	
}